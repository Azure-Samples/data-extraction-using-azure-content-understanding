# Example terraform.tfvars file
# Copy this file to terraform.tfvars and customize the values

# Azure subscription ID where resources will be deployed
subscription_id = "your-subscription-id-here"

# Azure region where resources will be deployed
location = "West US"

# Tags to apply to all resources
tags = {
  Environment = "development"
  Project     = "data-extraction-azure-ai"
  Owner       = "your-team"
  CostCenter  = "engineering"
}

# CosmosDB configuration
cosmos_consistency_level = "Session"

# AI Foundry service configuration
ai_foundry_sku = "S0"  # AI Foundry Hub only supports S0

# Storage account configuration
storage_account_tier = "Standard"
storage_replication_type = "LRS"
